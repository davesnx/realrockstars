{
  "main": "server.js",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "start": "nodemon --watch src --exec \"NODE_ENV=dev node build/server.js\"",
    "build": "concurrently --kill-others 'npm run build:client' 'npm run build:server'",
    "build:prod": "webpack --config webpack.client.js && webpack --config webpack.server.js",
    "build:client": "NODE_ENV=dev webpack --config webpack.client.js --watch",
    "build:server": "NODE_ENV=dev webpack --config webpack.server.js --watch"
  },
  "dependencies": {
    "approximate-number": "^2.0.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.3",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "html-react-parser": "^0.4.3",
    "node-fetch": "^2.1.2",
    "normalize.css": "^8.0.0",
    "octonode": "^0.9.2",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^16.3.2",
    "react-circular-progressbar": "^0.8.0",
    "react-dom": "^16.3.2",
    "react-feather": "^1.1.0",
    "react-helmet": "^5.2.0",
    "react-text-mask": "^5.3.0",
    "style-loader": "^0.21.0",
    "styled-components": "^3.2.6",
    "styled-normalize": "^4.0.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@sambego/storybook-styles": "^1.0.0",
    "@storybook/addon-options": "^3.4.2",
    "@storybook/cli": "^3.2.3",
    "@storybook/react": "^3.2.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "isomorphic-style-loader": "^4.0.0",
    "nodemon": "^1.17.3",
    "source-map-support": "^0.5.6",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
