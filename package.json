{
  "name": "realrockstars.io",
  "license": "BSD-3-Clause",
  "scripts": {
    "clean": "rm -rf build",
    "storybook": "start-storybook -p 6006",
    "dev": "nodemon --exec \"node build/server/bundle.js\"",
    "start": "npm run clean; concurrently 'npm run build:server:dev' 'npm run build:client:dev' 'npm run dev'",
    "serve": "node build/server/bundle.js",
    "build:client:dev": "NODE_ENV=development webpack --config webpack.client.js",
    "build:server:dev": "NODE_ENV=development webpack --config webpack.server.js",
    "build:server:prod": "NODE_ENV=production webpack --config webpack.server.js",
    "build:client:prod": "NODE_ENV=production webpack --config webpack.client.js"
  },
  "dependencies": {
    "approximate-number": "^2.0.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "emotion": "^9.2.5",
    "emotion-normalize": "^7.0.1",
    "emotion-server": "^9.2.5",
    "express": "^4.16.3",
    "git-url-parse": "^9.0.1",
    "node-fetch": "^2.1.2",
    "octonode": "^0.9.2",
    "prop-types": "^15.6.2",
    "ramda": "^0.24.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.2.5",
    "react-feather": "^1.1.1",
    "react-helmet": "^5.2.0",
    "react-spring": "^5.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-emotion": "^9.1.2",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "nodemon": "^1.18.0",
    "react-addons-css-transition-group": "^15.6.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
