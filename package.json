{
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf build",
    "storybook": "start-storybook -p 6006",
    "dev": "nodemon --watch build --exec \"node build/server/bundle.js\"",
    "start": "concurrently 'npm run build:client' 'npm run build:server' 'npm run dev'",
    "serve": "node build/server/bundle.js",
    "build:client": "webpack --config webpack.client.js --mode development --watch",
    "build:server": "webpack --config webpack.server.js --mode development --watch",
    "build": "webpack --config webpack.server.js --mode production",
    "prebuild": "webpack --config webpack.client.js --mode production"
  },
  "dependencies": {
    "approximate-number": "^2.0.0",
    "babel-plugin-emotion": "^9.1.2",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.3",
    "emotion": "^9.1.3",
    "emotion-normalize": "^7.0.1",
    "emotion-server": "^9.1.3",
    "express": "^4.16.3",
    "node-fetch": "^2.1.2",
    "octonode": "^0.9.2",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.2.3",
    "react-feather": "^1.1.0",
    "react-helmet": "^5.2.0",
    "react-spring": "^5.3.8",
    "react-text-mask": "^5.3.0",
    "styled-normalize": "^4.0.0"
  },
  "devDependencies": {
    "@sambego/storybook-styles": "^1.0.0",
    "@storybook/addon-options": "^3.4.2",
    "@storybook/cli": "^3.2.3",
    "@storybook/react": "^3.2.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "isomorphic-style-loader": "^4.0.0",
    "nodemon": "^1.17.3",
    "react-addons-css-transition-group": "^15.6.2",
    "source-map-support": "^0.5.6",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
