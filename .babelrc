{
  "ignore": ["node_modules"],
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 Chrome versions"]
        }
      }
    ]
  ],
  "plugins": [
    "ramda",
    "transform-async-to-generator",
    "transform-class-properties",
    "transform-object-rest-spread",
    "emotion"
  ]
}
