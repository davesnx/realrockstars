{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 2 Chrome versions"]
      }
    }]
  ],
  "plugins": [
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    "transform-async-to-generator",
    "transform-class-properties"
  ]
}
